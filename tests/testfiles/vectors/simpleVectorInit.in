//CHECK_FILE:simpleVectorInit.out

procedure main() returns integer {
    vector<character> vec1 = ['a', 'b'];
    var vector<boolean> vec2;
    var vector<integer> vec3;
    var vector<real> vec4;
    var vector<real> vec5;
    
    // array containers since vectors can't be printed
    var character[2] arr1;
    var boolean[2] arr2;
    var integer[3] arr3;
    var real[3] arr4;
    var real[3] arr5;



    // delayed init
    vec2 = [true, false];
    vec3 = [1, 2, 3];
    vec4 = [1.01, 2.0, 3.008];

    // assignment
    vec5 = vec4;

    // copy over results into arrays
    arr1 = vec1;
    arr2 = vec2;
    arr3 = vec3;
    arr4 = vec4;
    arr5 = vec5;

    // print results

    arr1 -> std_output;
    "\n" -> std_output;
    arr2 -> std_output;
    "\n" -> std_output;
    arr3 -> std_output;
    "\n" -> std_output;
    arr4 -> std_output;
    "\n" -> std_output;
    arr5 -> std_output;
    "\n" -> std_output;

    return 0;
}