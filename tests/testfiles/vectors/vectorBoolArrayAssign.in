procedure main() returns integer {
    var vector<boolean> vb;
    var boolean[4] b4;
    var boolean[2] b2;

    vb = [true, false];
    
    // Using cast to explicitely define behavior for size change
    b4 = as<boolean[4]>(vb);    // [T F F F]
    b2 = vb;                    // [T F] (Size matches)

    b4 -> std_output;
    "\n" -> std_output;
    b2 -> std_output;
    "\n" -> std_output;

    return 0;
}
