// no longer testing global structs bc initializers are scalar-only
struct Point (integer x, integer y);

procedure main() returns integer {
    const Point ORIGIN = Point(0, 0);
    Point p = ORIGIN;

    p.x -> std_output;
    " " -> std_output;
    p.y -> std_output;
    "\n" -> std_output;

    return 0;
}
