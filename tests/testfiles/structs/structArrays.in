procedure main() returns integer {
    struct Point (integer x, integer y);
    vector<Point> points;
    var Point p1;
    var Point p2;
    var Point temp;
    var Point check;
    
    // Initialize with literal
    points = [Point(1, 2), Point(3, 4)];
    
    // Read
    p1 = points[1];
    p1.x -> std_output;
    " " -> std_output;
    
    p2 = points[2];
    p2.y -> std_output;
    "\n" -> std_output;
    
    // Modify element via temp
    temp = points[1];
    temp.x = 10;
    points[1] = temp;
    
    // Verify
    check = points[1];
    check.x -> std_output;
    "\n" -> std_output;

    return 0;
}
