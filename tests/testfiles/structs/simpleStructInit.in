procedure main() returns integer {
    struct s1 (integer i, real r, boolean iv) t1 = (1, 3.14, true);
    struct Another (character char, real flo, boolean item);
    var Another t2;

    t2 = ('c', 9.9, false);

    "t1:\n" -> std_output;
    t1.i -> std_output;
    t1.r -> std_output;
    t1.iv -> std_output;

    "\n\nt2:\n" -> std_output;
    t2.char -> std_output;
    t2.flo -> std_output;
    t2.item -> std_output;
    "\n" -> std_output;

}

