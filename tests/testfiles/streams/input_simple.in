typealias integer int;

procedure main() returns int {
    var int x;
    var int y;

    "Enter one integer: " -> std_output;
    x <- std_input;

    y = x + 5;

    "Five added to " -> std_output;
    x -> std_output;
    " is " -> std_output;
    y -> std_output;

    return 0;
}

