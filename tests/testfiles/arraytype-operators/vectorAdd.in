procedure main() returns integer {
    vector<integer> vec1 = [
        1, 1, 1, 1
    ];
    var integer[4] arr1;

    vector<integer> vec2 = [
        5, 5, 5, 5
    ];
    var integer[4] arr2;
    var vector<integer> vec3;
    var integer[4] arr3;
    var vector<integer> vec4;
    var integer[4] arr4;

    arr1 = vec1;
    arr1 -> std_output;
    "\n" -> std_output;

    arr2 = vec2;
    arr2 -> std_output;
    "\n" -> std_output;

    vec3 = vec1 + vec2;

    arr3 = vec3;
    arr3 -> std_output;
    "\n" -> std_output;

    vec4 = vec1 - [4, 4, 4, 4];
    arr4 = vec4;
    arr4 -> std_output;
    "\n" -> std_output;

    return 0;
}

